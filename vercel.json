{
    "headers": [
        {
            "source": "/sw.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        },
        {
            "source": "(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=86400, max-age=86400"
                },
                {
                    "key": "Vercel-CDN-Cache-Control",
                    "value": "max-age=31536000"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/404",
            "dest": "/404.html"
        },
        {
            "src": "/(.*)",
            "dest": "/404.html",
            "status": 404
        }
    ]
}